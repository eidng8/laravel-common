{
  "name": "eidng8/laravel-utils",
  "description": "Utilities for use with Laravel.",
  "keywords": [
    "utility",
    "laravel"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "eidng8/common": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "squizlabs/php_codesniffer": "^3.2",
    "fzaninotto/faker": "^1.7",
    "orchestra/testbench": "3.6.*"
  },
  "autoload": {
    "psr-4": {
      "edign8\\Laravel\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "edign8\\Tests\\Laravel\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "edign8\\Laravel\\Providers\\MacroServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
